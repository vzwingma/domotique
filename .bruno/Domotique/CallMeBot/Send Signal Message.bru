meta {
  name: Send Signal Message
  type: http
  seq: 1
}

get {
  url: https://api.callmebot.com/signal/send.php?phone={{callmebot-phone}}&apikey={{callmebot-apikey}}&text=Message
  body: none
  auth: inherit
}

params:query {
  phone: {{callmebot-phone}}
  apikey: {{callmebot-apikey}}
  text: Message
}

headers {
  Content-Type: application/json
}

settings {
  encodeUrl: true
  timeout: 0
}
